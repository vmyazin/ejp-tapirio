{
  "version": 2,
  "builds": [
    {
      "src": "./app.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 10
      }
    },
    {
      "src": "./public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/stylesheets/(.*)",
      "dest": "/public/stylesheets/$1"
    },
    {
      "src": "/javascript/(.*)",
      "dest": "/public/javascript/$1"
    },
    {
      "src": "/(.*)\\.(xml|json|svg|png|jpg|jpeg|gif|ico|css|js)",
      "dest": "/public/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ]
}